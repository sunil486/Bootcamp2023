package com.sunil486.tranx.spring_transactional_01;

import org.springframework.context.ConfigurableApplicationContext;
import org.springframework.context.annotation.AnnotationConfigApplicationContext;

import com.sunil486.tranx.spring_transactional_01.config.ProductConfig;
import com.sunil486.tranx.spring_transactional_01.dto.Product;
import com.sunil486.tranx.spring_transactional_01.service.ProductService;

public class App {
	public static void main(String[] args) {
		ConfigurableApplicationContext context = new AnnotationConfigApplicationContext(ProductConfig.class);

		Product product = context.getBean("productService", ProductService.class);
	}
}
